    "plugins": [
      "@babel/transform-runtime",
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ]
    ]
  },
  "scripts": {
    "build": "$(npm bin)/build-storybook -s public -c .storybook -o build",
